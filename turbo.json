{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "public/build/**"]
    },
    "lint": {},
    "format": {},
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "coverage": {
      "dependsOn": ["test"],
      "outputs": ["coverage/**"]
    }
  }
}